{
  "$schema": "https://schema.management.azure.com/schemas/2019-04-01/deploymentParameters.json#",
  "contentVersion": "1.0.0.0",
  "parameters": {
    "_artifactsLocation": {
      "value": "https://raw.githubusercontent.com/DCMattyG/azure-projects/master/dns-forwarder/"
    },
    "vmName": {
      "value": "dns-forward"
    },
    "userName": {
      "value": "AzureAdmin"
    },
    "userPass": {
      "value": "<vm-password-here>"
    },
    "vmNetwork": {
      "value": "Cloud-NET"
    },
    "vmSubnet": {
      "value": "Infrastructure"
    },
    "vmNetRG": {
      "value": "Network-RG"
    },
    "vmPrivateIP": {
      "value": "10.0.0.100"
    },
    "vmPublicIP": {
      "value": true
    },
    "trustedEndpoints": {
        "value": [
            "172.16.0.100"
        ]
    }
  }
}
